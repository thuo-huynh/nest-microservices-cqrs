{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api-gateway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
  },
  "projects": {
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    },
    "account-command": {
      "type": "application",
      "root": "apps/account-command",
      "entryFile": "main",
      "sourceRoot": "apps/account-command/src",
      "compilerOptions": {
        "tsConfigPath": "apps/account-command/tsconfig.app.json"
      }
    },
    "account-query": {
      "type": "application",
      "root": "apps/account-query",
      "entryFile": "main",
      "sourceRoot": "apps/account-query/src",
      "compilerOptions": {
        "tsConfigPath": "apps/account-query/tsconfig.app.json"
      }
    }
  },
  "monorepo": true,
  "root": "apps/api-gateway"
}